{
  "name": "styled-components-to-stylex-codemod",
  "version": "0.0.4",
  "description": "Codemod to transform styled-components to StyleX",
  "keywords": [
    "codemod",
    "css-in-js",
    "jscodeshift",
    "styled-components",
    "stylex",
    "transform"
  ],
  "license": "MIT",
  "author": {
    "name": "Kenneth Skovhus",
    "email": "kenneth.skovhus@gmail.com",
    "url": "https://github.com/skovhus"
  },
  "repository": "http://github.com/skovhus/styled-components-to-stylex-codemod",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint --type-aware --deny-warnings",
    "lint:fix": "oxfmt && pnpm lint --fix",
    "typecheck:src": "tsc --noEmit",
    "typecheck:fixtures": "tsc -p test-cases/tsconfig.json --noEmit",
    "typecheck": "pnpm run typecheck:src && pnpm run typecheck:fixtures",
    "ci": "pnpm run lint && pnpm run typecheck && pnpm run test:run && knip",
    "knip": "knip",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "playground": "vite --config playground/vite.config.ts",
    "build:playground": "vite build --config playground/vite.config.ts"
  },
  "dependencies": {
    "jscodeshift": "^17.3.0",
    "stylis": "^4.3.2"
  },
  "devDependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@emotion/is-prop-valid": "^1.4.0",
    "@storybook/react": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@stylexjs/babel-plugin": "^0.17.4",
    "@stylexjs/stylex": "^0.17.4",
    "@stylexjs/unplugin": "^0.17.4",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/stylis": "^4.2.5",
    "@uiw/react-codemirror": "^4.25.4",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "assert": "^2.1.0",
    "knip": "^5.81.0",
    "lefthook": "^2.0.13",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.1",
    "path-browserify": "^1.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.1.10",
    "styled-components": "^6.1.19",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=22.20",
    "pnpm": ">=10.22.0"
  },
  "packageManager": "pnpm@10.22.0"
}
