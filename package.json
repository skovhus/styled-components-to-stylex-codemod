{
  "name": "styled-components-to-stylex-codemod",
  "version": "0.0.0",
  "description": "Codemod to transform styled-components to StyleX",
  "keywords": [
    "codemod",
    "css-in-js",
    "jscodeshift",
    "styled-components",
    "stylex",
    "transform"
  ],
  "license": "MIT",
  "author": "",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./transform": {
      "types": "./dist/transform.d.mts",
      "import": "./dist/transform.mjs"
    },
    "./run": {
      "types": "./dist/run.d.mts",
      "import": "./dist/run.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint --type-aware --deny-warnings ",
    "lint:fix": "oxfmt && pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "ci": "pnpm run lint && pnpm run typecheck && pnpm run test:run",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "jscodeshift": "^17.3.0",
    "stylis": "^4.3.2"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@playwright/test": "^1.57.0",
    "@storybook/react": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@stylexjs/babel-plugin": "^0.17.4",
    "@stylexjs/stylex": "^0.17.4",
    "@stylexjs/unplugin": "^0.17.4",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/stylis": "^4.2.7",
    "@vitejs/plugin-react": "^5.1.2",
    "lefthook": "^2.0.13",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.1.10",
    "styled-components": "^6.1.19",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=22.20",
    "pnpm": ">=10.22.0"
  },
  "packageManager": "pnpm@10.22.0"
}
